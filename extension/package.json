{
  "name": "ducklab",
  "displayName": "Ducklab (preview)",
  "description": "Interactive SQL Notebooks. Powered by DuckDB",
  "version": "0.3.2",
  "license": "Apache-2.0",
  "publisher": "ducklake",
  "icon": "./logo.png",
  "repository": "https://github.com/HassaanAkbar/ducklab",
  "scripts": {
    "build": "npm run build-rollup && npm run post-build",
    "build-rollup": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "post-build": "cd dist && npm i duckdb-async && npm run package",
    "package": "vsce package",
    "dev": "rollup --watch --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest"
  },
  "engines": {
    "vscode": "^1.58.0"
  },
  "keywords": [
    "sql",
    "notebooks",
    "duckdb",
    "ducklab",
    "notebookKernelIsql"
  ],
  "categories": [
    "Data Science",
    "Notebooks"
  ],
  "extensionDependencies": [
    "vscode.sql",
    "vscode.python",
    "ms-toolsai.jupyter"
  ],
  "type": "module",
  "main": "./dist/build/index.cjs",
  "activationEvents": [
    "onNotebook:ipynb-ducklab",
    "onNotebook:isql"
  ],
  "contributes": {
    "notebooks": [
      {
        "type": "isql",
        "displayName": "Ducklab",
        "selector": [
          {
            "filenamePattern": "*.isql"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "ducklab.listKernels",
        "title": "Select a kernel",
        "category": "DuckLab"
      }
    ],
    "menus": {
      "notebook/toolbar": [
        {
          "command": "ducklab.listKernels",
          "title": "MenuItem - Execute code against a Python Kernel"
        }
      ]
    }
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.5.0",
    "@types/vscode": "^1.58.0",
    "@types/vscode-notebook-renderer": "^1.72.3",
    "@vscode/jupyter-extension": "^1.1.1",
    "@vscode/vsce": "^3.0.0",
    "jest": "^29.7.0",
    "rollup": "^4.20.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@ducklab/core": "0.3.11",
    "@nteract/messaging": "^7.0.20",
    "apache-arrow": "^17.0.0",
    "aws-sdk": "^2.1685.0",
    "duckdb-async": "^1.0.0",
    "get-port": "^7.1.0",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.5.5",
    "rxjs": "^7.8.1",
    "split2": "^4.2.0",
    "vscode-notebook-error-overlay": "^1.0.1"
  }
}